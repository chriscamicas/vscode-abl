{
	"extends": "tslint:recommended",
	"rules": {
        "max-line-length": false,
		"indent": [
			true,
			"spaces",
			4
        ],
        "interface-name": false,
		"no-duplicate-variable": true,
		"quotemark": [
			true,
			"single"
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format"
        ]
	}
}